<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tongji.programming.mapper.StudentMapper">
    <select id="selectByIdPass" resultType="org.tongji.programming.pojo.Student">
            select * from student where stu_no = #{id} and stu_password = MD5(#{password}) limit 1
    </select>
</mapper>
